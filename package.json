{
  "name": "cbb-language-support",
  "displayName": "C Double-Flat Language Support",
  "description": "",
  "author": {
    "name": "Harry Allen"
  },
  "publisher": "HarryAllen",
  "version": "1.1.0",
  "icon": "cbb_logo.png",
  "engines": {
    "vscode": "^1.64.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "vsce package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MajesticString/cbb-language-support"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "contributes": {
    "languages": [
      {
        "id": "cbb",
        "aliases": [
          "C Double-Flat",
          "cbb"
        ],
        "extensions": [
          ".cbb"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "cbb_logo.png",
          "dark": "cbb_logo.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "cbb",
        "scopeName": "source.cbb",
        "path": "./syntaxes/cbb.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "cbb",
        "path": "./snippets.json"
      }
    ]
  },
  "devDependencies": {
    "vsce": "^2.14.0"
  }
}
